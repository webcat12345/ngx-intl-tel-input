{
  "name": "ngx-intl-tel-input-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build_stats": "ng build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/ngx-intl-tel-input-app/stats.json",
    "build_lib": "ng build ngx-intl-tel-input",
    "copy-license": "cp ./LICENSE ./dist/ngx-intl-tel-input",
    "copy-readme": "cp ./README.md ./dist/ngx-intl-tel-input",
    "copy-files": "npm run copy-license && npm run copy-readme",
    "npm_pack": "npm run copy-files && cd dist/ngx-intl-tel-input && npm pack",
    "package": "rimraf ./dist && npm run build_lib && npm run npm_pack",
    "ci:publish": "npm run build_lib && npm run semantic-release",
    "semantic-release": "semantic-release",
    "postversion": "cp -r package.json ../../projects/ngx-intl-tel-input",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.0",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.0.0",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "bootstrap": "^5.2.3",
    "core-js": "^3.6.5",
    "google-libphonenumber": "^3.2.3",
    "intl-tel-input": "^17.0.3",
    "ngx-bootstrap": "^10.0.0",
    "popper.js": "^1.16.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.0",
    "@angular/cli": "^15.0.0",
    "@angular/compiler-cli": "^15.0.0",
    "@angular/language-service": "^15.0.0",
    "@types/google-libphonenumber": "^7.4.23",
    "@types/jasmine": "~3.8.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "ajv": "^8.11.2",
    "eslint": "^7.2.0",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.8",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^2.0.6",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^15.0.0",
    "prettier": "^2.0.5",
    "protractor": "^7.0.0",
    "rollup": "^2.22.0",
    "semantic-release": "^18.0.0",
    "ts-node": "~7.0.0",
    "typescript": "~4.8.2",
    "webpack-bundle-analyzer": "^3.9.0"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "dist/ngx-intl-tel-input"
        }
      ],
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": "projects/ngx-intl-tel-input/package.json",
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
